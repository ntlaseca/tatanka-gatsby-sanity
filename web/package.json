{
  "private": false,
  "name": "tatanka-gatsby-sanity",
  "description": "Tatanka Travel offers luxury travel arrangements for those who love adventure.",
  "version": "1.0.0",
  "author": "Nate Tlaseca <nate@whitepantsagency.com>",
  "repository": {
    "type": "git",
    "url": "git://github.com/ntlaseca/tatanka-gatsby-sanity.git"
  },
  "license": "MIT",
  "engines": {
    "node": "15.x"
  },
  "scripts": {
    "build": "gatsby build",
    "deploy": "npm run now-deploy",
    "develop": "gatsby develop",
    "empty-cache": "gatsby clean",
    "lint": "eslint .",
    "now-build": "npm run build",
    "workers-build": "npm run build",
    "workers-start": "npm run build && wrangler preview --watch",
    "workers-deploy": "npm run build && wrangler publish",
    "now-dev": "npm run build",
    "start": "npm run develop",
    "format": "prettier-eslint --write \"**/*.js\" \"!.cache/**\" \"!node_modules/**\" \"!public/**\"",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\""
  },
  "dependencies": {
    "@cloudflare/wrangler": "^1.13.0",
    "@sanity/block-content-to-react": "^2.0.7",
    "@sanity/image-url": "^0.140.22",
    "autoprefixer": "^10.2.4",
    "axios": "^0.21.1",
    "date-fns": "^1.30.1",
    "dotenv": "^8.2.0",
    "gatsby": "^2.32.8",
    "gatsby-image": "^2.11.0",
    "gatsby-plugin-google-analytics": "^2.11.0",
    "gatsby-plugin-manifest": "^2.12.1",
    "gatsby-plugin-postcss": "^3.0",
    "gatsby-plugin-react-helmet": "^3.10.0",
    "gatsby-plugin-sharp": "^2.14.3",
    "gatsby-source-filesystem": "^2.11.1",
    "gatsby-source-sanity": "^6.0.5",
    "gatsby-transformer-sharp": "^2.12.0",
    "katex": "^0.11.1",
    "postcss": "^8.2.6",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.7.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-helmet": "^5.2.1",
    "react-instagram-embed": "^2.0.0",
    "react-player": "^2.9.0",
    "sanity-mux-player": "0.0.27"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-standard": "^4.1.0",
    "prettier-eslint-cli": "^5.0.0",
    "prop-types": "^15.7.2"
  }
