{
  "private": false,
  "name": "tatanka-gatsby-sanity-studio",
  "version": "1.0.0",
  "author": "Nate Tlaseca <nate@whitepantsagency.com>",
  "engines": {
    "node": "10.x"
  },
  "scripts": {
    "deploy": "npm run graphql-deploy && npm run sanity-deploy",
    "format": "prettier-eslint --write \"**/*.js\" \"!node_modules/**\"",
    "graphql-deploy": "sanity graphql deploy --playground",
    "sanity-deploy": "sanity deploy",
    "lint": "eslint .",
    "start": "sanity start",
    "build": "sanity build",
    "now-build": "npm run build",
    "workers-start": "npm run build && wrangler preview --watch",
    "workers-deploy": "npm run build && wrangler publish",
    "now-dev": "npm run build",
    "test": "sanity check"
  },
  "dependencies": {
    "@sanity/base": "^2.4.0",
    "@sanity/cli": "^2.4.0",
    "@sanity/code-input": "^2.2.6",
    "@sanity/color-input": "^2.2.6",
    "@sanity/components": "^2.2.6",
    "@sanity/core": "^2.4.0",
    "@sanity/dashboard": "^2.2.6",
    "@sanity/default-layout": "^2.4.0",
    "@sanity/default-login": "^2.2.6",
    "@sanity/desk-tool": "^2.4.0",
    "@sanity/production-preview": "^2.2.6",
    "@sanity/vision": "^2.2.6",
    "katex": "^0.11.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-icons": "^3.11.0",
    "react-instagram-embed": "^1.5.0",
    "react-player": "^1.15.2",
    "sanity-plugin-asset-source-thispersondoesnotexist": "0.0.1",
    "sanity-plugin-asset-source-unsplash": "^0.1.3",
    "sanity-plugin-color-list": "^2.0.1",
    "sanity-plugin-dashboard-widget-document-list": "^0.0.11",
    "sanity-plugin-dashboard-widget-netlify": "^1.0.1",
    "sanity-plugin-google-analytics": "^0.2.6",
    "sanity-plugin-media": "^0.4.0",
    "sanity-plugin-mux-input": "^0.2.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-standard": "^4.0.1",
    "prettier-eslint-cli": "^5.0.0"
  }
}
